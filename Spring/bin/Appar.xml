<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
               
  <bean id ="Welcome" class="Collections.Community">
  <property name="house">
  <list>
  <ref  bean="House1"/>
  <ref  bean="House2"/>
  
  </list>
  
  
  
  </property>
  
  
  
  </bean>             
               
  <bean id= "House1" class="Collections.House">
 <property name="app1" value="822"/>
 <property name="roomNo" value="304"/>
 <property name="bedroom" value="2BHK"/>
  </bean>
               
               
 <bean id= "House2" class="Collections.House">
 <property name="app1" value="823"/>
 <property name="roomNo" value="301"/>
 <property name="bedroom" value="1BHK"/>
  </bean>              
  
  
 </beans>