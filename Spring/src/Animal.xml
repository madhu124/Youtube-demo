<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
<bean id="mamals" class="LifeCycleOfbean.Animal" init-method="print" destroy-method="FinalMethod">
<property name="age" value="3"></property>
<property name="name" value="Dog"></property>
<property name="cruelAnimal">

<list>
<ref bean="c1"/>
<ref bean="c2"/>


</list>




</property>

</bean>               

<bean id="c1" class="LifeCycleOfbean.CruelAnimal"> 
<property name="age" value="4"></property>
<property name="name" value="cow"></property>
</bean>               
 
 <bean id="c2" class="LifeCycleOfbean.CruelAnimal"> 
<property name="age" value="5"></property>
<property name="name" value="cat"></property>
</bean>              
               
               
 <bean class="LifeCycleOfbean.PostProcessor"></bean>
               
               
               
<bean id="one" class="LifeCycleOfbean.CruelAnimal">

<property name="age" value="10"></property>
<property name="name" value="pig"></property>

</bean>               


</beans>